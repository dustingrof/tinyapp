<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-dark">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
 <% if (userObject) { %>
      <li class="nav-item">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
      </li>
      <li class="nav-item">
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </li>
    <% } %>
    </ul>
<% if (!userObject) { %>
        <a class="nav-item nav-link" href="/register">Register</a>
        <a class="nav-item nav-link btn btn-secondary" href="/login">Login</a>
        <% } %> <% if (userObject) { %>
        <a class="nav-item nav-link">Logged in as: <%= userObject.email %></a>
        <form class="form-inline" action="/logout" method="POST">
          <div class="form-group mb-2">
            <button type="submit" class="btn btn-secondary">Logout</button>
          </div>
        </form>
        <% } %>
        
  </nav>
</header>